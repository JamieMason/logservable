{
  "name": "logservable",
  "description": "git log as an observable stream of JSON objects",
  "version": "4.0.0",
  "author": "Jamie Mason <jamie@foldleft.io> (https://github.com/JamieMason)",
  "bugs": "https://github.com/JamieMason/logservable/issues",
  "devDependencies": {
    "@types/node": "12.0.8",
    "prettier": "1.16.4",
    "rimraf": "2.6.3",
    "rxjs": "6.4.0",
    "tslint": "5.13.1",
    "typescript": "3.3.3333"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/JamieMason/logservable",
  "keywords": [
    "git",
    "git-log",
    "rxjs"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "peerDependencies": {
    "rxjs": "6.x.x"
  },
  "repository": "JamieMason/logservable",
  "scripts": {
    "build": "tsc --project .",
    "format": "prettier --write '**/*.md' './src/**/*.ts'",
    "lint": "tslint --fix --project ./tsconfig.json",
    "prebuild": "rimraf ./dist",
    "prelint": "npm run format",
    "prepack": "npm run build"
  },
  "typings": "dist/index.d.ts"
}
